
<h1>Edit user</h1>
 
<%= form_for(@user) do |f| %>
 
  
  <p>
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </p>
 
  <p>
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name %>
  </p>
 <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit %>
  </p>
 
<% end %>
 


<%= link_to 'Back', :action => nil %>